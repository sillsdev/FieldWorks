<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<FwBuildTasksAssembly Condition="'$(FwBuildTasksAssembly)'==''"
			>$(MSBuildThisFileDirectory)..\BuildTools\FwBuildTasks\$(Configuration)\FwBuildTasks.dll</FwBuildTasksAssembly
		>
	</PropertyGroup>
	<UsingTask TaskName="CatenateFiles" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Clouseau" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="ComputerName" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="CpuArchitecture" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="DownloadFile" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="ForceDelete" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="GenerateFwTargets" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="GenerateNUnitReports" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="GenerateTestCoverageReport" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="GetSpecialFolderPath" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="LogMetadata" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Make" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Md5Checksum" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="NUnit3" AssemblyFile="SIL.BuildTasks.dll" />
	<UsingTask TaskName="ParentDirectory" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="ParseVersionNumbers" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="PathCombine" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="RegFree" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="RegisterForTests" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="SetEnvVar" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Substitute" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Unitpp" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Unzip" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="VerifyFilesIdentical" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="WriteRegistry" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask TaskName="Zip" AssemblyFile="$(FwBuildTasksAssembly)" />
	<UsingTask
		TaskName="CheckAdminPrivilege"
		AssemblyFile="$(FwBuildTasksAssembly)"
		Condition="'$(OS)'=='Windows_NT'"
	/>
	<!-- REVIEW (Hasso) 2022.11: replaceable by SIL.BuildTasks.MakeWixForDirTree? -->
	<UsingTask
		TaskName="WxsToWxi"
		AssemblyFile="$(FwBuildTasksAssembly)"
		Condition="'$(OS)'=='Windows_NT'"
	/>
</Project>
