	<!-- STUFF ADDED FOR NEW CONFIGURATION WORK -->
	<!--
		Naming Convention:
		Layout names are laid out like this: publish<Type>(Minor)<Field>
		For instance, publishStem or publishRoot for the top-level layout called by the Language Explorer tool.
					  publishStemEntry - layout for a full entry in a stem-based dictionary
					  publishStemMinorEntry - layout for a minor entry in a stem-based dictionary
					  publishStemPara - sublayout containing paragraph contents for a full entry
					  publishStemMinorPara - sublayout containing paragraph contents for a minor entry

		Part names end with either Pub (for string and other simple types) or Config (for fields that invoke other layouts).
		Some part names for simple type fields may just be the field name without either ending.
	-->

	<layout class="LexEntry" type="jtview" name="publishStem">
		<part ref="StemConfig" param="publishStem{0}Entry"/>
	</layout>
	<layout class="LexEntry" type="jtview" name="publishRoot">
		<part ref="RootConfig" param="publishRoot{0}Entry"/>
	</layout>

	<layout class="LexEntry" type="jtview" name="publishStemPreview">
		<part ref="StemConfig" param="publishStem{0}EntryPreview"/>
	</layout>
	<layout class="LexEntry" type="jtview" name="publishRootPreview">
		<part ref="RootConfig" param="publishRoot{0}EntryPreview"/>
	</layout>

