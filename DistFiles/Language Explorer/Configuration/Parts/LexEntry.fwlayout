  <layout class="LexEntry" type="detail" name="Normal">
	<part label="Lexeme Form" ref="LexemeForm"/>
	<part label="Citation Form" ref="CitationFormAllV"/>
	<part ref="ComplexFormEntries" visibility="ifdata"/>
	<part ref="EntryRefs" param="Normal" visibility="ifdata"/>
