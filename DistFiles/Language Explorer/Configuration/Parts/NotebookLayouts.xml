	<!-- ADDED FOR DOCUMENT VIEW -->

	<layout class="RnGenericRec" type="jtview" name="publishRecord">
		<part ref="TitlePub" label="Title" visibility="always" flowType="para" style="Heading 1" />
		<part ref="AnthroCodesConfig" label="Anthropology Categories" param="publishRnAnthroCode" flowType="para" style="Normal" before="Anthropology Categories: " beforeStyle="Strong" sep=", " after="." visibility="ifdata" />
		<part ref="DescriptionConfig" param="publishRnDescription" hideConfig="true"/>
		<part ref="HypothesisConfig" param="publishRnHypothesis" hideConfig="true"/>
		<part ref="DiscussionConfig" param="publishRnDiscussion" hideConfig="true"/>
		<part ref="ConclusionsConfig" param="publishRnConclusions" hideConfig="true"/>
		<part ref="FurtherQuestionsConfig" param="publishRnFurtherQuestions" hideConfig="true"/>
		<part ref="ResearchPlanConfig" param="publishRnResearchPlan" hideConfig="true"/>
		<part ref="PersonalNotesConfig" param="publishRnPersonalNotes" hideConfig="true"/>
		<part ref="ClassificationsConfig" label="Classifications" param="publishClassifications" flowType="para" style="Normal"/>
		<part ref="ExternalMaterialsConfig" param="publishRnExternalMaterials" hideConfig="true"/>
		<part ref="SupportingEvidenceConfig" label="Supporting Evidence" param="publishRnSupportingEvidence" flowType="para" style="Normal" before="Supporting Evidence: " beforeStyle="Strong" sep=", " visibility="ifdata"/>
		<part ref="CounterEvidenceConfig" label="Counter Evidence" param="publishRnCounterEvidence" flowType="para" style="Normal" before="Counter Evidence: " beforeStyle="Strong" sep=", " visibility="ifdata"/>
		<part ref="SupersededByConfig" label="Superseded By" param="publishRnSupersededBy" flowType="para" style="Normal" before="Superseded By: " beforeStyle="Strong" sep=", " visibility="ifdata"/>
		<part ref="SeeAlsoConfig" label="See Also" param="publishRnSeeAlso" flowType="para" style="Normal" before="See Also: " beforeStyle="Strong" sep=", " visibility="ifdata"/>
		<part ref="HistoryConfig" label="Created/Modified" param="publishHistory" flowType="para" style="Normal" visibility="never"/>
		<generate class="RnGenericRec" fieldType="atom" restrictions="customOnly" destClass="StText">
			<part ref="CustomStTextForRecord_$fieldName" hideConfig="true" param="publishCustomStText"/>
		</generate>
		<part ref="SubrecordsConfig" label="Subrecords" param="publishRecord" flowType="div" before="Subrecords" beforeStyle="Heading 2" visibility="ifdata" css="subentries" recurseConfig="false" number="%O"/>
	</layout>

