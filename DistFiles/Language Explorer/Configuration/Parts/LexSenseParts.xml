	<bin class="LexReference">
		<part id="LexReference-Jt-AbbreviationPub" type="jtview">
			<choice>
				<where field="Targets" lengthatleast="1">
					<choice>
						<where target="owner" field="MappingType" intmemberof="0,1,5,6,10,11,4,9,14">
							<!-- Collections and simple pairs, Sequences -->
							<configureMlString target="owner" field="Abbreviation" />
						</where>
						<where target="owner" field="MappingType" intmemberof="2,3,7,8,12,13">
							<!-- Trees and assymetrical pairs  -->
							<if field="Targets" index="0" hvoequals="$ParentHvo">
								<configureMlString target="owner" field="Abbreviation" />
							</if>
							<ifnot field="Targets" index="0" hvoequals="$ParentHvo">
								<configureMlString target="owner" field="ReverseAbbreviation" />
							</ifnot>
						</where>
					</choice>
				</where>
			</choice>
		</part>
		<part id="LexReference-Jt-NamePub" type="jtview">
			<choice>
				<where field="Targets" lengthatleast="1">
					<choice>
						<where target="owner" field="MappingType" intmemberof="0,1,5,6,10,11,4,9,14">
							<!-- Collections and simple pairs, Sequences -->
							<configureMlString target="owner" field="Name" />
						</where>
						<where target="owner" field="MappingType" intmemberof="2,3,7,8,12,13">
							<!-- Trees and assymetrical pairs  -->
							<if field="Targets" index="0" hvoequals="$ParentHvo">
								<configureMlString target="owner" field="Name" />
							</if>
							<ifnot field="Targets" index="0" hvoequals="$ParentHvo">
								<configureMlString target="owner" field="ReverseName" />
							</ifnot>
						</where>
					</choice>
				</where>
			</choice>
		</part>
		<part id="LexReference-Jt-TargetsPub" type="jtview">
			<choice>
				<where field="Targets" lengthatleast="2">
					<choice>
						<where target="owner" field="MappingType" intmemberof="0,1,5,6,10,11">
							<!-- Collections and simple pairs, leave out the current item -->
							<seq field="Targets" layout="CrossRefPub" inheritSeps="true" excludeHvo="parent" />
						</where>
						<where target="owner" field="MappingType" intmemberof="2,3,7,8,12,13">
							<!-- Trees and assymetrical pairs  -->
							<if field="Targets" index="0" hvoequals="$ParentHvo">
								<!-- the first entry in Targets (root) is the current object, so exclude it -->
								<seq field="Targets" layout="CrossRefPub" inheritSeps="true" excludeHvo="parent" />
							</if>
							<ifnot field="Targets" index="0" hvoequals="$ParentHvo">
								<!-- The current one is not the first, so show only the first -->
								<seq field="Targets" layout="CrossRefPub" firstOnly="true" />
							</ifnot>
						</where>
						<where target="owner" field="MappingType" intmemberof="4,9,14">
							<!-- Sequences; show everything, including the current one. -->
							<seq field="Targets" layout="CrossRefPub" inheritSeps="true" />
						</where>
					</choice>
				</where>
				<where field="Targets" lengthatleast="1" lengthatmost="1">
					<if target="owner" field="MappingType" intmemberof="4,9,14">
						<!-- Sequences -->
						<span>
							<properties>
								<italic value="on" />
							</properties>
							<string target="owner" field="Abbreviation" ws="analysis" />
							<lit>: </lit>
						</span>
						<seq field="Targets" layout="CrossRefName" sep=", " />
					</if>
				</where>
			</choice>
		</part>
	</bin>
