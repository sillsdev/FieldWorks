<?xml version="1.0" encoding="UTF-8" ?>
<!-- Manually created Parts for LexSense. -->
<PartInventory>
	<bin class="LexSense">
		<part id="LexSense-Jt-Summary" type="jtview">
			<para>
				<properties>
					<forecolor value="ControlDarkDark" />
					<fontsize value="10000"/>
					<style value="UiElement"/>
					<bold value="on" />
				</properties>
				<lit>Sense </lit>
				<string field="LexSenseOutline"></string>
			</para>
		</part>
		<part id="LexSense-Jt-MLHeadWord" type="jtview">
			<string field="MLOwnerOutlineName"  ws="$ws=vernacular"/>
		</part>
		<part id="LexSense-Jt-SummaryCollapsed" type="jtview">
			<para>
				<properties>
					<forecolor value="ControlDarkDark" />
					<fontsize value="10000"/>
					<bold value="on" />
				</properties>
				<span>
					<properties>
						<style value="UiElement"/>
					</properties>
					<lit>Sense </lit>
					<string field="LexSenseOutline"></string>
					<lit> - </lit>
				</span>
				<choice>
					<where field="Definition" stringaltequals="" ws="all analysis">
						<string field="Gloss" ws="best analysis"/>
					</where>
					<otherwise>
						<string field="Definition" ws="best analysis"/>
					</otherwise>
				</choice>
				<lit> - </lit>
				<obj field="MorphoSyntaxAnalysis" layout="Basic"/>
			</para>
		</part>
		<part id="LexSense-Jt-publicationPictures" type="jtview">
			<seq field="Pictures" layout="publication" sep="  " />
		</part>
		<part id="CmPossibility-Jt-BoldItalicAbbr" type="jtview">
			<span>
				<properties>
					<italic value="on" />
					<bold value="on" />
				</properties>
				<string field="Abbreviation" ws="analysis" />
				<lit>.</lit>
			</span>
		</part>
		<part id="LexSense-Detail-ExampleList" type="Detail">
			<seq field="Examples" layout="ExampleAllV" ghost="Example" ghostWs="vernacular" />
		</part>
		<part id="LexSense-Jt-DefAnalysisInPara" type="jtview">
			<para>
				<string field="Gloss" ws="analysis" />
			</para>
		</part>
		<part id="LexSense-Jt-DefinitionInPara" type="jtview">
			<para>
				<string field="Definition" ws="analysis" />
			</para>
		</part>
		<part id="LexSense-Jt-OwningEntry" type="jtview">
			<obj class="LexSense" field="OwningEntry"/>
		</part>
		<part id="LexSense-Jt-DomainsAbbrAndName" type="jtview">
	  <span>
		<properties>
		  <editable value="false"/>
		</properties>
		<seq field="SemanticDomains" layout="AbbrAndName" sep="; "/>
	  </span>
	</part>
		<part id="LexSense-Jt-Gloss" type="jtview">
			<string field="Gloss" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-Definition" type="jtview">
			<string field="Definition" ws="analysis"/>
		</part>
	<!-- ForEntryOrSense parts -->
		<!-- Generic Fields for PartGenerator -->
		<!-- Sense -> Entry custom field path part -->
		<part id="LexSense-Jt-$fieldName" type="jtview">
			<obj class="LexSense" field="OwningEntry" layout="$fieldName"/>
		</part>
		<!-- Sense custom field parts -->
		<part id="LexSense-Jt-CustomMultiStringForSense_$fieldName" type="jtview">
			<string field="$fieldName" ws="$wsName"/>
		</part>
		<part id="LexSense-Jt-CustomSimpleStringForSense_$fieldName" type="jtview">
			<string field="$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomIntegerForSense_$fieldName" type="jtview">
			<int field="$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomGenDateForSense_$fieldName" type="jtview">
			<gendate field="$fieldName" format="short"/>
		</part>
		<part id="LexSense-Jt-CustomStTextForSense_$fieldName" type="jtview">
			<obj field="$fieldName" layout="FullText"/>
		</part>
		<part id="LexSense-Jt-CustomPossVectorForSense_$fieldName" type="jtview">
			<seq field="$fieldName" layout="PossibilityName"/>
		</part>
		<part id="LexSense-Jt-CustomPossAtomForSense_$fieldName" type="jtview">
			<obj field="$fieldName" layout="Name"/>
		</part>
		<!-- Sense -> Example Sentence custom field path parts -->
		<part id="LexSense-Jt-CustomMultiStringForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomMultiStringForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomSimpleStringForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomSimpleStringForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomIntegerForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomIntegerForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomGenDateForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomGenDateForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomStTextForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomStTextForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomPossVectorForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomPossVectorForExample_$fieldName"/>
		</part>
		<part id="LexSense-Jt-CustomPossAtomForExample_$fieldName" type="jtview">
			<seq field="Examples" layout="CustomPossAtomForExample_$fieldName"/>
		</part>

		<!-- Specified Fields -->
		<part id="LexSense-Jt-SenseHeadwordForSense" type="jtview">
			<para>
				<properties>
					<editable value="false"/>
				</properties>
				<string field="MLOwnerOutlineName" ws="$ws=vernacular"/>
			</para>
		</part>
		<part id="LexSense-Jt-GrammaticalInfoFullForSense" type="jtview">
			<obj field="MorphoSyntaxAnalysis" layout="Browse" />
		</part>
		<part id="LexSense-Jt-GrammaticalInfoAbbrForSense" type="jtview">
			<obj field="MorphoSyntaxAnalysis" layout="BrowseAbbr" />
		</part>
		<part id="LexSense-Jt-GrammaticalCategoryForSense" type="jtview">
			<para>
				<properties>
					<editable value="false"/>
				</properties>
				<obj class="LexSense" field="MorphoSyntaxAnalysis" layout="empty">
					<if is="MoStemMsa">
						<obj class="MoStemMsa" field="PartOfSpeech" layout="empty">
							<string class="CmPossibility" field="Name" ws="$ws=best analysis"/>
						</obj>
					</if>
					<ifnot is="MoStemMsa">
						<lit>n/a</lit>
					</ifnot>
				</obj>
			</para>
		</part>
		<part id="LexSense-Jt-GlossesForSense" type="jtview">
			<para>
				<string class="LexSense" field="Gloss" ws="analysis"/>
			</para>
		</part>
		<part id="LexSense-Jt-DefinitionsForSense" type="jtview">
			<para>
				<string class="LexSense" field="Definition" ws="analysis"/>
			</para>
		</part>
		<part id="LexSense-Jt-ReversalsForSense" type="jtview">
			<string field="ReversalEntriesBulkText" ws="$ws=reversal"/>
		</part>
		<part id="LexSense-Jt-InflectionClassForStem" type="jtview">
			<obj class="LexSense" field="MorphoSyntaxAnalysis" layout="empty">
				<if is="MoStemMsa">
					<obj class="MoStemMsa" field="InflectionClass" layout="empty">
						<string class="MoInflClass" field="Name" ws="$ws=best analysis"/>
					</obj>
				</if>
				<ifnot is="MoStemMsa">
					<lit>n/a</lit>
				</ifnot>
			</obj>
		</part>
		<part id="LexSense-Jt-InflectionFeaturesForSense" type="jtview">
			<obj class="LexSense" field="MorphoSyntaxAnalysis" layout="empty">
				<if is="MoStemMsa">
					<obj class="MoStemMsa" field="MsFeatures" layout="empty">
						<string field="ChooserNameTS"/>
					</obj>
				</if>
				<ifnot is="MoStemMsa">
					<lit>n/a</lit>
				</ifnot>
			</obj>
		</part>
		<part id="LexSense-Jt-ExceptionFeatures" type="jtview">
			<obj class="LexSense" field="MorphoSyntaxAnalysis" layout="empty">
				<if is="MoStemMsa">
					<seq class="MoStemMsa" field="ProdRestrict" layout="Name"/>
				</if>
				<if is="MoDerivAffMsa">
					<seq class="MoDerivAffMsa" field="FromProdRestrict"
					layout="Name"/>
					<if lengthatleast="1" field="FromProdRestrict">
						<lit>&gt;</lit>
					</if>
					<ifnot lengthatleast="1" field="FromProdRestrict">
						<if lengthatleast="1" field="ToProdRestrict">
							<lit>&gt;</lit>
						</if>
					</ifnot>
					<seq class="MoDerivAffMsa" field="ToProdRestrict" layout="Name"/>
				</if>
				<if is="MoInflAffMsa">
					<seq class="MoInflAffMsa" field="FromProdRestrict" layout="Name"/>
				</if>
				<ifnot is="MoStemMsa">
					<ifnot is="MoDerivAffMsa">
						<ifnot is="MoInflAffMsa">
							<lit>n/a</lit>
						</ifnot>
					</ifnot>
				</ifnot>
			</obj>
		</part>
		<part id="LexSense-Jt-PictureCaptionForSense" type="jtview">
			<seq field="Pictures" layout="empty">
				<para>
					<string field="Caption" ws="vernacular analysis"/>
				</para>
			</seq>
		</part>
		<part id="LexSense-Jt-PictureFileNameForSense" type="jtview">
			<seq field="Pictures" layout="empty">
				<para>
					<string field="PathNameTSS" ws="$ws=analysis"/>
				</para>
			</seq>
		</part>
		<part id="LexSense-Jt-StatusForSense" type="jtview">
			<obj class="LexSense" field="Status" layout="empty">
				<string field="Name" ws="best analysis"/>
			</obj>
		</part>
		<part id="LexSense-Jt-AnthropologyNoteForSense" type="jtview">
			<string class="LexSense" field="AnthroNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-BibliographyForSense" type="jtview">
			<string field="Bibliography" ws="$ws=analysis"/>
		</part>
		<part id="LexSense-Jt-DiscourseNoteForSense" type="jtview">
			<string class="LexSense" field="DiscourseNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-EncyclopedicInfoForSense" type="jtview">
			<string class="LexSense" field="EncyclopedicInfo" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-GeneralNoteForSense" type="jtview">
			<string class="LexSense" field="GeneralNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-GrammarNoteForSense" type="jtview">
			<string class="LexSense" field="GrammarNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-ImportResidueSense" type="jtview">
			<string class="LexSense" field="ImportResidue" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-PhonologyNoteForSense" type="jtview">
			<string class="LexSense" field="PhonologyNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-RestrictionsForSense" type="jtview">
			<string class="LexSense" field="Restrictions" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-ScientificNameForSense" type="jtview">
			<string class="LexSense" field="ScientificName"/>
		</part>
		<part id="LexSense-Jt-SemanticsNoteForSense" type="jtview">
			<string class="LexSense" field="SemanticsNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-SociolinguisticsNoteForSense" type="jtview">
			<string class="LexSense" field="SocioLinguisticsNote" ws="analysis"/>
		</part>
		<part id="LexSense-Jt-SourceForSense" type="jtview">
			<string class="LexSense" field="Source"/>
		</part>
		<part id="LexSense-Jt-AnthropologyCategoriesForSense" type="jtview">
	  <seq field="AnthroCodes" layout="PossibilityAbbrAndName"/>
	</part>
		<part id="LexSense-Jt-AcademicDomainsForSense" type="jtview">
			<seq field="DomainTypes" layout="Name" sep=", "/>
		</part>
		<part id="LexSense-Jt-SenseTypeForSense" type="jtview">
			<obj class="LexSense" field="SenseType" layout="empty">
				<string field="Name" ws="best analysis"/>
			</obj>
		</part>
		<part id="LexSense-Jt-UsagesForSense" type="jtview">
			<seq field="UsageTypes" layout="Name" sep=", "/>
		</part>
		<part id="LexSense-Jt-PublishInForSenses" type="JtView">
			<seq field="PublishIn" layout="Name" />
		</part>
		<part id="LexSense-Jt-PublishInForExamples" type="JtView">
			<seq field="Examples" layout="PublishInForExamples"/>
		</part>
		<part id="LexSense-Jt-Example" type="jtview">
			<seq field="Examples" layout="Example"/>
		</part>
		<part id="LexSense-Jt-ExampleReference" type="jtview">
			<seq field="Examples" layout="ExampleReference"/>
		</part>
		<part id="LexSense-Jt-ExampleTranslation" type="jtview">
			<seq field="Examples" layout="ExampleTranslation"/>
		</part>
		<part id="LexSense-Jt-ExampleTranslationType">
			<seq field="Examples" layout="ExampleTranslationType"/>
		</part>
		<part id="LexSense-Jt-LexicalRelationsForSense" type="jtview">
			<seq field="MinimalLexReferences" layout="ShowInPara"/>
		</part>
		<part id="LexSense-Jt-DomainsOfSensesForSense" type="jtview">
			<para>
				<seq field="SemanticDomains" layout="AbbrAndName" sep="; "/>
			</para>
		</part>
		<part id="LexSense-Jt-ReversalEntriesForSense" type="jtview">
			<para>
		<seq field="ReversalEntries" layout="ReversalForm" sep="; " checkForEmptyItems="true"/>
			</para>
		</part>
	<part id="LexSense-Jt-ReversalEntriesForSenseEdit" type="jtview">
	  <para>
		<string field="ReversalEntriesBulkText" ws="$ws=reversal"/>
	  </para>
	</part>
	<!-- end of ForEntryOrSense parts -->

		<part id="LexSense-Jt-MsaInPara" type="jtview">
			<obj field="MorphoSyntaxAnalysis" layout="Browse" />
		</part>
		<part id="LexSense-Jt-MsaInParaAbbr" type="jtview">
			<obj field="MorphoSyntaxAnalysis" layout="BrowseAbbr" />
		</part>

	<part id="LexSense-Jt-PictureCaption" type="jtview">
	  <seq field="Pictures" layout="empty">
		<para>
		  <string field="Caption" ws="best analysis"/>
		</para>
	  </seq>
	</part>
	<part id="LexSense-Jt-PictureFileName" type="jtview">
	  <seq field="Pictures" layout="empty">
		<para>
		  <string field="PathNameTSS" ws="$ws=analysis"/>
		</para>
	  </seq>
	</part>

	<part id="LexSense-Jt-RootComplexFormsConfig">
		<seq field="VisibleComplexFormBackRefs" layout="publishRootComplexFormRef" inheritSeps="true" targetclass="LexEntryRef" sort="ascending"/>
	</part>
	<!-- *********************************************************************************** -->
		<!-- Overrides of otherwise generated parts -->
		<!-- *********************************************************************************** -->
		<!-- force indent for subsenses because same time of major object -->
		<part id="LexSense-Detail-Senses" type="Detail">
			<seq field="Senses" indent="true" />
		</part>
		<part id="LexSense-Detail-UsageTypes" type="Detail">
			<slice field="UsageTypes" label="Usages" editor="possVectorReference" menu="mnuDataTree-Help">
				<deParams displayProperty="ShortName" ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the list of Usages." tool="usageTypeEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-ReversalEntries" type="detail">
			<slice field="ReversalEntries" label="Reversal Entries" editor="Custom" assemblyPath="LexEdDll.dll"
				class="SIL.FieldWorks.XWorks.LexEd.ReversalIndexEntrySlice" menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-SenseType" type="Detail">
			<slice field="SenseType" label="Sense Type" editor="possAtomicReference" menu="mnuDataTree-Help">
				<deParams ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the Sense Types" tool="senseTypeEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-DomainTypes" type="Detail">
			<slice field="DomainTypes" label="Academic Domains" editor="possVectorReference" menu="mnuDataTree-Help">
				<deParams displayProperty="ShortName" ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the Academic Domains" tool="domainTypeEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-SemanticDomains" type="Detail">
			<slice field="SemanticDomains" label="Semantic Domains" editor="possVectorReference"
				menu="mnuDataTree-Help">
				<deParams displayProperty="AbbrAndNameTSS" ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the Semantic Domains" tool="semanticDomainEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-PublishIn" type="Detail">
			<slice field="PublishIn" label="Publish In" tooltip="Remove a publication from this field to completely exclude this sense from that publication." editor="defaultVectorReference" visField="DoNotPublishIn"
				menu="mnuDataTree-Help">
				<deParams displayProperty="ShortName" ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the Publications list" tool="publicationsEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-AnthroCodes" type="Detail">
			<slice field="AnthroCodes" label="Anthropology Categories" editor="possVectorReference"
				menu="mnuDataTree-Help">
				<deParams displayProperty="LongName" ws="best analysis" />
				<chooserInfo>
					<chooserLink type="goto" label="Edit the Anthropology Categories" tool="anthroEdit" />
				</chooserInfo>
			</slice>
		</part>
		><part id="LexSense-Detail-Status" type="Detail">
			<slice field="Status" label="Status" editor="possAtomicReference" menu="mnuDataTree-Help">
				<deParams ws="best analysis" />
				<chooserInfo title="Choose Status">
					<chooserLink type="goto" label="Edit the Status list" tool="statusEdit" />
				</chooserInfo>
			</slice>
		</part>
		<part id="LexSense-Detail-Source" type="Detail">
			<slice field="Source" label="Source" editor="String" menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-ScientificName" type="Detail">
			<slice field="ScientificName" label="ScientificName" editor="String" menu="mnuDataTree-Help" />
		</part>
		<!-- *********************************************************************************** -->
		<part id="LexSense-Detail-SenseSummary" type="detail">
			<slice id="LexSenseId" editor="jtview" expansion="expanded" menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-GlossAllA" type="detail">
			<slice field="Gloss" label="Gloss" editor="multistring" ws="all analysis" menu="mnuDataTree-Help" sideEffectMethod="AdjustDerivedAnalysis">
				<properties>
					<bold value="on" />
					<fontsize value="100%" />
				</properties>
			</slice>
		</part>

		<part id="LexSense-Jt-GlossIfNoDefn" type="detail">
			<if field="Definition" stringaltequals="" ws="analysis">
				<ifnot field="Gloss" stringaltequals="" ws="analysis">
					<string field="Gloss" ws="analysis" />
				</ifnot>
			</if>
		</part>

		<part id="LexSense-Detail-DefinitionAllA" type="detail">
			<slice field="Definition" label="Definition" editor="multistring" ws="all analysis" menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-MsaCombo" type="detail">
			<slice field="MorphoSyntaxAnalysis" label="Grammatical Info." abrev="gfn" editor="msaReferenceComboBox"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-AnthroNoteAllA" type="detail">
			<slice field="AnthroNote" label="Anthropology Note" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-BibliographyAllA" type="detail">
			<slice field="Bibliography" label="Bibliography" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-DiscourseNoteAllA" type="detail">
			<slice field="DiscourseNote" label="Discourse Note" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-EncyclopedicInfoAllA" type="detail">
			<slice field="EncyclopedicInfo" label="Encyclopedic Info" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-GeneralNoteAllA" type="detail">
			<slice field="GeneralNote" label="General Note" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-GrammarNoteAllA" type="detail">
			<slice field="GrammarNote" label="Grammar Note" editor="multistring" ws="all analysis"
				menu="mnuDataTree-Help" />
		</part>
		<part id="LexSense-Detail-ComplexFormEntries" type="Detail">
			<slice field="ComplexFormEntries" label="Complex Forms" menu="mnuDataTree-Help"
				   editor="custom" assemblyPath="LexEdDll.dll" class="SIL.FieldWorks.XWorks.LexEd.EntrySequenceReferenceSlice">
				<deParams displayProperty="HeadWord"/>
			</slice>
		</part>
		<part id="LexSense-Jt-ComplexFormsNotSubentries">
			<seq field="ComplexFormsNotSubentries" inheritSeps="true" targetclass="LexEntryRef" sort="ascending"/>
		</part>
		<part id="LexSense-Detail-VisibleComplexFormEntries" type="Detail">
			<slice field="VisibleComplexFormBackRefs" label="Referenced Complex Forms" tooltip="Complex forms shown in this field are shown as references in this component entry in Dictionary." menu="mnuReorderVector" reorder="true"
				   editor="custom" assemblyPath="LexEdDll.dll" class="SIL.FieldWorks.XWorks.LexEd.EntrySequenceReferenceSlice" helpTopicID="khtpField-ReferencedComplexForms-EntryLevel">
				<deParams displayProperty="HeadWord"/>
			</slice>
		</part>
		<part id="LexSense-Detail-Subentries" type="Detail">
			<slice field="Subentries" label="Subentries" tooltip="Show these subentries below this sense in root-based dictionary views." menu="mnuDataTree-Help"
				   editor="custom" assemblyPath="LexEdDll.dll" class="SIL.FieldWorks.XWorks.LexEd.EntrySequenceReferenceSlice">
				<deParams displayProperty="HeadWord"/>
			</slice>
		</part>
