			</xsl:when>
			<xsl:when test="name()='MoExoCompound'">
			   <xsl:if test="$ToPartOfSpeech">                                | percolation
		&lt;Stem_1 synCat&gt;       = <xsl:value-of select="$ToPartOfSpeech"/>
				  <!--        &lt;Stem_1 inflectonClass&gt;       = <xsl:value-of select="$ToInflectionClass"/> -->
