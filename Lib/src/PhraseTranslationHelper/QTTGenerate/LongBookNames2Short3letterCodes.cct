begin > utf8
	store(bknm) endstore use(main)
	store(num) '0123456789' endstore
	store(chvsbrk) ':' endstore

group(main)
nl '\tqref '  > dup use(abbreviate)

nl '\tqref ' fol(num) fol(chvsbrk) 		> next
nl '\tqref ' fol(num) fol(num) fol(chvsbrk) 	> dup out(bknm) ' ' use(abbreviate)

group(abbreviate)

