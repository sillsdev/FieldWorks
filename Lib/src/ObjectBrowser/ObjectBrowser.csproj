<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>ObjectBrowser</AssemblyName>
    <RootNamespace>SIL.ObjectBrowser</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="WeifenLuo.WinFormsUI.Docking, Version=2.3.1.15724, Culture=neutral, processorArchitecture=MSIL" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\Src\CommonAssemblyInfo.cs" />
    <Compile Include="ColorPicker.cs" SubType="UserControl" />
    <Compile Include="InspectorGrid.cs" SubType="Component" />
    <Compile Include="ObjectBrowser.cs" SubType="Form" />
    <Compile Include="InspectorWnd.cs" SubType="Form" />
    <Compile Include="OptionsDlg.cs" SubType="Form" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ColorPicker.resx" DependentUpon="ColorPicker.cs" />
    <EmbeddedResource Include="ObjectBrowser.resx" DependentUpon="ObjectBrowser.cs" />
    <EmbeddedResource Include="InspectorWnd.resx" DependentUpon="InspectorWnd.cs" />
    <EmbeddedResource Include="OptionsDlg.resx" DependentUpon="OptionsDlg.cs" />
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Properties\Settings.settings" Generator="SettingsSingleFileGenerator" LastGenOutput="Settings.Designer.cs" />
  </ItemGroup>
</Project>
