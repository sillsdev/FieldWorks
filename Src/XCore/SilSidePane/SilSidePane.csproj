<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>SilSidePane</AssemblyName>
    <RootNamespace>SIL.SilSidePane</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../Common/ViewsInterfaces/ViewsInterfaces.csproj" />
    <ProjectReference Include="../../Common/RootSite/RootSite.csproj" />
    <ProjectReference Include="../xCore.csproj" />
    <ProjectReference Include="../xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../Utilities/XMLUtils/XMLUtils.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Core" />
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
    <Compile Include="Banner.cs" SubType="Component" />
    <Compile Include="ListViewItemArea.cs" SubType="Component" />
    <Compile Include="NavPaneOptionsDlg.cs" SubType="Form" />
    <Compile Include="OutlookBar.cs" SubType="Component" />
    <Compile Include="OutlookButtonPanel.cs" SubType="Component" />
    <Compile Include="OutlookBarSubButtonPanel.cs" SubType="Component" />
    <Compile Include="OutlookButtonPanelItemArea.cs" SubType="Component" />
    <Compile Include="SidePane.cs" SubType="Component" />
    <Compile Include="StripListItemArea.cs" SubType="Component" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="NavPaneOptionsDlg.resx" DependentUpon="NavPaneOptionsDlg.cs" />
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
    <EmbeddedResource Include="SilSidePane.resx" Generator="ResXFileCodeGenerator" LastGenOutput="SilSidePane.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Properties\Settings.settings" Generator="SettingsSingleFileGenerator" LastGenOutput="Settings1.Designer.cs" />
    <None Include="Resources\Arrow_Down.ico" />
    <None Include="Resources\Arrow_Up.ico" />
    <None Include="Resources\DefaultIcon.ico" />
    <None Include="Resources\DropDown2003.ico" />
    <None Include="Resources\DropDown2007.ico" />
    <None Include="Resources\Grip2003.ico" />
    <None Include="Resources\Grip2007.ico" />
  </ItemGroup>
</Project>