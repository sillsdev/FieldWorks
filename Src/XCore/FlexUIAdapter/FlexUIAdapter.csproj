    <Compile Include="PanelButton.cs" SubType="Component" />
    <Compile Include="PanelMenu.cs" SubType="Component" />
    <Compile Include="SidebarAdapter.cs" SubType="Component" />
    <Compile Include="ToolbarAdapter.cs" SubType="Component" />
