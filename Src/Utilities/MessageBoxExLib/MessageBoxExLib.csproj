    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
    <Compile Include="MessageBoxExForm.cs" SubType="Form" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="MessageBoxExForm.resx" DependentUpon="MessageBoxExForm.cs" />
