<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>Reporting</AssemblyName>
    <RootNamespace>SIL.Utils</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.Core.Desktop" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <Reference Include="System.Core" />
    <Reference Include="System" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
    <Compile Include="ErrorReport.cs" SubType="Form" />
    <Compile Include="UsageEmailDialog.cs" SubType="Form" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="ErrorReport.resx" DependentUpon="ErrorReport.cs" />
    <EmbeddedResource Include="ReportingStrings.resx" Generator="ResXFileCodeGenerator" LastGenOutput="ReportingStrings.Designer.cs" />
    <EmbeddedResource Include="UsageEmailDialog.resx" DependentUpon="UsageEmailDialog.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Resources\cc.png" />
    <None Include="App.config" />
  </ItemGroup>
</Project>
