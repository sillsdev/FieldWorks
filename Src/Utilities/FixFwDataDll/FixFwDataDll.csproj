<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>FixFwDataDll</AssemblyName>
    <RootNamespace>SIL.FieldWorks.FixData</RootNamespace>
    <TargetFramework>net48</TargetFramework>
    <OutputType>Library</OutputType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PlatformTarget>x64</PlatformTarget>
    <Prefer32Bit>false</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|x64' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>portable</DebugType>
    <Optimize>false</Optimize>
    <DefineConstants>DEBUG;TRACE</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|x64' ">
    <DebugType>portable</DebugType>
    <Optimize>true</Optimize>
    <DefineConstants>TRACE</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SIL.LCModel" Version="11.0.0-*" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-*" />
    <PackageReference Include="SIL.LCModel.FixData" Version="11.0.0-*" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-*" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="netstandard" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../../FwCoreDlgs/FwCoreDlgs.csproj" />
    <ProjectReference Include="../../FwResources/FwResources.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="FixFwDataStrings.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>FixFwDataStrings.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="FixFwDataStrings.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>FixFwDataStrings.resx</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>
