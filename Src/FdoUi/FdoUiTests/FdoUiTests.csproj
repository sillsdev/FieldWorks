<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>FdoUiTests</AssemblyName>
    <RootNamespace>SIL.FieldWorks.FdoUi</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../FdoUi.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtilsTests/FwUtilsTests.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Core.Tests" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Tests" Version="11.0.0-beta0135" />
    <PackageReference Include="CommonServiceLocator" Version="2.0.7" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.TestUtilities" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.WritingSystems" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils.Tests" Version="11.0.0-beta0135" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="nunit.framework" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\AssemblyInfoForTests.cs" />
  </ItemGroup>
</Project>