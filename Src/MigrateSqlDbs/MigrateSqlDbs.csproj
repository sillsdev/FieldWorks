<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>WinExe</OutputType>
    <AssemblyName>MigrateSqlDbs</AssemblyName>
    <RootNamespace>SIL.FieldWorks.MigrateSqlDbs.MigrateProjects</RootNamespace>
    <OutputPath>../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../Common/FwUtils/FwUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.WritingSystems" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Data" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\CommonAssemblyInfo.cs" />
    <Compile Include="FWVersionTooOld.cs" SubType="Form" />
    <Compile Include="MigrateProjects.cs" SubType="Form" />
    <Compile Include="ExistingProjectDlg.cs" SubType="Form" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="FWVersionTooOld.resx" DependentUpon="FWVersionTooOld.cs" />
    <EmbeddedResource Include="MigrateProjects.resx" DependentUpon="MigrateProjects.cs" />
    <EmbeddedResource Include="ExistingProjectDlg.resx" DependentUpon="ExistingProjectDlg.cs" />
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Properties\Settings.settings" Generator="PublicSettingsSingleFileGenerator" LastGenOutput="Settings.Designer.cs" />
    <None Include="Resources\Icon1.ico" />
    <None Include="Resources\BookOnCube.ico" />
  </ItemGroup>
</Project>
