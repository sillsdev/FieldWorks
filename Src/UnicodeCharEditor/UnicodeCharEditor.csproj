<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>WinExe</OutputType>
    <AssemblyName>UnicodeCharEditor</AssemblyName>
    <RootNamespace>SIL.FieldWorks.UnicodeCharEditor</RootNamespace>
    <OutputPath>../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../Utilities/Reporting/Reporting.csproj" />
    <ProjectReference Include="../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../FwCoreDlgs/FwCoreDlgs.csproj" />
    <ProjectReference Include="../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../Utilities/XMLUtils/XMLUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="CommandLineArgumentsParser" Version="3.0.22" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <PackageReference Include="icu.net" Version="3.0.0-beta.297" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Xml.Linq" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\CommonAssemblyInfo.cs" />
    <Compile Include="CharEditorWindow.cs" SubType="Form" />
    <Compile Include="CustomCharDlg.cs" SubType="Form" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="CharEditorWindow.resx" DependentUpon="CharEditorWindow.cs" />
    <EmbeddedResource Include="HelpTopicPaths.resx" Generator="ResXFileCodeGenerator" LastGenOutput="HelpTopicPaths.Designer.cs" />
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
    <EmbeddedResource Include="CustomCharDlg.resx" DependentUpon="CustomCharDlg.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Properties\Settings.settings" Generator="SettingsSingleFileGenerator" LastGenOutput="Settings.Designer.cs" />
  </ItemGroup>
</Project>
