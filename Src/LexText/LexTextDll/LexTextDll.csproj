<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>LexTextDll</AssemblyName>
    <RootNamespace>SIL.FieldWorks.XWorks.LexText</RootNamespace>
    <TargetFramework>net48</TargetFramework>
    <OutputType>Library</OutputType>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701,NU1903</NoWarn>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PlatformTarget>x64</PlatformTarget>
    <Prefer32Bit>false</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|x64' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>false</Optimize>
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|x64' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>true</Optimize>
    <DebugType>portable</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="CommonServiceLocator" Version="2.0.7" Exclude="Build,Analyzers" />
    <PackageReference Include="SIL.Core" Version="17.0.0-*" />
    <PackageReference Include="SIL.Core.Desktop" Version="17.0.0-*" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-*" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-*" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-*" />
    <PackageReference Include="SIL.DesktopAnalytics" Version="4.0.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="netstandard" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="../../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../../Common/Controls/XMLViews/XMLViews.csproj" />
    <ProjectReference Include="../../Common/Framework/Framework.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../../Common/RootSite/RootSite.csproj" />
    <ProjectReference Include="../../FwCoreDlgs/FwCoreDlgs.csproj" />
    <ProjectReference Include="../../Utilities/XMLUtils/XMLUtils.csproj" />
    <ProjectReference Include="../../XCore/xCore.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../xWorks/xWorks.csproj" />
    <ProjectReference Include="../Interlinear/ITextDll.csproj" />
    <ProjectReference Include="../LexTextControls/LexTextControls.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="LexTextDllTests/**" />
    <None Remove="LexTextDllTests/**" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="icons\Add New Analysis.ico" />
    <EmbeddedResource Include="icons\Find a Wordform.ico" />
    <EmbeddedResource Include="LT.ico" />
  </ItemGroup>
</Project>
