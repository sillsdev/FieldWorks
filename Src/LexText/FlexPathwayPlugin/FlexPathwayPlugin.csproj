<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>FlexPathwayPlugin</AssemblyName>
    <RootNamespace>SIL.PublishingSolution</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../FwCoreDlgs/FwCoreDlgs.csproj" />
    <ProjectReference Include="../../FwResources/FwResources.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../../Utilities/Reporting/Reporting.csproj" />
    <ProjectReference Include="../../Common/RootSite/RootSite.csproj" />
    <ProjectReference Include="../../XCore/xCore.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../Utilities/XMLUtils/XMLUtils.csproj" />
    <ProjectReference Include="../../xWorks/xWorks.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
  </ItemGroup>
</Project>