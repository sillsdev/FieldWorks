<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>WinExe</OutputType>
    <AssemblyName>Flex</AssemblyName>
    <RootNamespace>SIL.FieldWorks.XWorks.LexText</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../Common/FieldWorks/FieldWorks.csproj" />
    <ProjectReference Include="../../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../LexTextDll/LexTextDll.csproj" />
    <ProjectReference Include="../ParserUI/ParserUI.csproj" />
    <ProjectReference Include="../../Common/ViewsInterfaces/ViewsInterfaces.csproj" />
    <ProjectReference Include="../../XCore/FlexUIAdapter/FlexUIAdapter.csproj" />
    <ProjectReference Include="../../Common/Framework/Framework.csproj" />
    <ProjectReference Include="../../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../../Common/RootSite/RootSite.csproj" />
    <ProjectReference Include="../../XCore/xCore.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../xWorks/xWorks.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Core" />
    <Reference Include="netstandard" />
    <Reference Include="System" />
    <Reference Include="System.Data" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
  </ItemGroup>
</Project>