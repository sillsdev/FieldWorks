<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>MGA</AssemblyName>
    <RootNamespace>SIL.FieldWorks.LexText.Controls.MGA</RootNamespace>
    <OutputPath>../../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../../Common/Controls/FwControls/FwControls.csproj" />
    <ProjectReference Include="../../../Common/FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../../../Utilities/XMLUtils/XMLUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="CommonServiceLocator" Version="2.0.7" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.WritingSystems" Version="15.0.0-beta0117" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\CommonAssemblyInfo.cs" />
    <Compile Include="GlossListBox.cs" SubType="Component" />
    <Compile Include="GlossListTreeView.cs" SubType="Component" />
    <Compile Include="MGADialog.cs" SubType="Form" />
    <Compile Include="MGAHtmlHelpDialog.cs" SubType="Form" />
    <Compile Include="PhonologicalFeaturesTreeView.cs" SubType="Component" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="CheckBox.bmp" />
    <EmbeddedResource Include="CheckedBox.bmp" />
    <EmbeddedResource Include="CLSDFOLD.BMP" />
    <EmbeddedResource Include="Complex.bmp" />
    <EmbeddedResource Include="FSType.bmp" />
    <EmbeddedResource Include="GlossListBox.resx" DependentUpon="GlossListBox.cs" />
    <EmbeddedResource Include="MGADialog.resx" DependentUpon="MGADialog.cs" />
    <EmbeddedResource Include="MGAStrings.resx" Generator="ResXFileCodeGenerator" LastGenOutput="MGAStrings.Designer.cs" />
    <EmbeddedResource Include="OPENFOLD.BMP" />
    <EmbeddedResource Include="Radio.bmp" />
    <EmbeddedResource Include="RadioSelected.bmp" />
    <EmbeddedResource Include="UserChoice.bmp" />
  </ItemGroup>
</Project>