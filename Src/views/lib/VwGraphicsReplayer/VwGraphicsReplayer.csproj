    <Reference Include="System.Drawing" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\CommonAssemblyInfo.cs" />
    <Compile Include="VwGraphicsReplayer.cs" SubType="Form" />
  </ItemGroup>
</Project>
