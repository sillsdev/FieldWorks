<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>RootSite</AssemblyName>
    <RootNamespace>SIL.FieldWorks.Common.RootSites</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../ViewsInterfaces/ViewsInterfaces.csproj" />
    <ProjectReference Include="../../FwResources/FwResources.csproj" />
    <ProjectReference Include="../FwUtils/FwUtils.csproj" />
    <ProjectReference Include="../SimpleRootSite/SimpleRootSite.csproj" />
    <ProjectReference Include="../UIAdapterInterfaces/UIAdapterInterfaces.csproj" />
    <ProjectReference Include="../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Accessibility" />
    <PackageReference Include="SIL.Core.Desktop" Version="15.0.0-beta0117" />
    <Reference Include="netstandard" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="CommonServiceLocator" Version="2.0.7" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.WritingSystems" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <PackageReference Include="icu.net" Version="3.0.0-beta.297" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
    <Compile Include="RootSite.cs" SubType="UserControl" />
    <Compile Include="RootSiteControl.cs" SubType="UserControl" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
    <EmbeddedResource Include="RootSite.resx" DependentUpon="RootSite.cs" />
    <EmbeddedResource Include="RootSiteControl.resx" DependentUpon="RootSiteControl.cs" />
    <EmbeddedResource Include="RootSiteStrings.resx" Generator="ResXFileCodeGenerator" LastGenOutput="RootSiteStrings.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="Resources\footnote" />
  </ItemGroup>
</Project>
