<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
    <AssemblyName>FwUtils</AssemblyName>
    <RootNamespace>SIL.FieldWorks.Common.FwUtils</RootNamespace>
    <OutputPath>../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../ViewsInterfaces/ViewsInterfaces.csproj" />
    <ProjectReference Include="../../FwResources/FwResources.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CommonServiceLocator" Version="2.0.7" />
    <PackageReference Include="NAudio" Version="1.10.0" />
    <Reference Include="netstandard" />
    <PackageReference Include="NAudio.Lame" Version="1.1.5" />
    <PackageReference Include="SIL.Core.Desktop" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.Windows.Forms" Version="15.0.0-beta0117" />
    <Reference Include="System.Configuration" />
    <Reference Include="System.Data" />
    <Reference Include="System.Management" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
    <Reference Include="System.Xml.Linq" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <PackageReference Include="icu.net" Version="3.0.0-beta.297" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.ServiceModel" />
    <Reference Include="System.Web" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\CommonAssemblyInfo.cs" />
    <Compile Include="FlexHelpProvider.cs" SubType="Component" />
    <Compile Include="FwUpdateChooserDlg.cs" SubType="Form" />
    <Compile Include="OLEConvert.cs" SubType="Component" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="FwUpdateChooserDlg.resx" DependentUpon="FwUpdateChooserDlg.cs" />
    <EmbeddedResource Include="FwUtilsStrings.resx" Generator="ResXFileCodeGenerator" LastGenOutput="FwUtilsStrings.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="app.config" />
    <None Include="BuildInclude.targets" />
    <None Include="Properties\Settings.settings" Generator="SettingsSingleFileGenerator" LastGenOutput="Settings.Designer.cs" />
  </ItemGroup>
</Project>
