<?xml version='1.0' encoding='utf-8'?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>SimpleRootSiteTests</AssemblyName>
    <RootNamespace>SIL.FieldWorks.Common.RootSites.SimpleRootSiteTests</RootNamespace>
    <OutputPath>../../../../Output/$(Configuration)</OutputPath>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <UseWindowsForms>true</UseWindowsForms>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>168,169,219,414,649,1635,1702,1701,1685</NoWarn>
    <WarningLevel>4</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DefineConstants>DEBUG;TRACE</DefineConstants>
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>TRACE</DefineConstants>
    <DebugSymbols>false</DebugSymbols>
    <DebugType>none</DebugType>
    <Optimize>true</Optimize>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="../../../CacheLight/CacheLight.csproj" />
    <ProjectReference Include="../../../CacheLight/CacheLightTests/CacheLightTests.csproj" />
    <ProjectReference Include="../../ViewsInterfaces/ViewsInterfaces.csproj" />
    <ProjectReference Include="../SimpleRootSite.csproj" />
    <ProjectReference Include="../../../XCore/xCoreInterfaces/xCoreInterfaces.csproj" />
    <ProjectReference Include="../../FwUtils/FwUtilsTests/FwUtilsTests.csproj" />
    <ProjectReference Include="../../FwUtils/FwUtils.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="SIL.LCModel.Core.Tests" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.TestUtilities" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.Windows.Forms.Keyboarding" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils.Tests" Version="11.0.0-beta0135" />
    <Reference Include="netstandard" />
    <Reference Include="nunit.framework" />
    <PackageReference Include="SIL.Core" Version="8.1.0-beta0035" />
    <PackageReference Include="SIL.WritingSystems" Version="15.0.0-beta0117" />
    <PackageReference Include="SIL.LCModel.Utils" Version="11.0.0-beta0135" />
    <PackageReference Include="SIL.LCModel.Core" Version="11.0.0-beta0135" />
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Windows.Forms" />
    <PackageReference Include="ibusdotnet" Version="2.0.3" />
    <PackageReference Include="NDesk.DBus" Version="0.15.0" />
    <Reference Include="Rhino.Mocks" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\..\AssemblyInfoForTests.cs" />
    <Compile Include="SimpleBasicView.cs" SubType="UserControl" />
    <Compile Include="SimpleRootSiteDataProviderView.cs" SubType="UserControl" />
    <Compile Include="SimpleRootSiteTests_IsSelectionVisibleTests.cs" SubType="UserControl" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="SimpleBasicView.resx" DependentUpon="SimpleBasicView.cs" />
    <EmbeddedResource Include="Properties\Resources.resx" Generator="ResXFileCodeGenerator" LastGenOutput="Resources.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="TextCacheModel.xml" />
  </ItemGroup>
</Project>