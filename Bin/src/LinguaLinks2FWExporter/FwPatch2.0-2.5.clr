val June-18-2002 13:42:52 editor : application subclass of ModalDialogApplication showing (\
\
	pane 'main' showing for all app in owner of my owner editable pile showing (\
\
		lead with height = 7,\
\
		regenerate row showing (\
			show 'Media File:' with (\
				text.bold = true,\
				edit.selectWholeTarget = true,\
				target.selectable = false\
				),\
			button labeled 'Browse...' performing\
				begin\
					var file\
					file := do getFilePathName ('Choose Media File', [ 'All files', 'AV', 'Graphics', 'MP3' ] ) to !System\
					if exists of ^file then\
						begin\
							do convertToPortable to ^file\
							set path of ^application to ^file\
						end\
				end\
				with text.bold = true,\
			relativePathName of path of ^app using listItem with (\
				edit.selectWholeTarget = true,\
				target.selectable = false\
				)\
			) with row.tabs = (51) on changes to path of ^app,\
\
		paragraph showing (\
			show 'Choose file with one of the following types:'\
			) with (\
			edit.selectWholeTarget = true,\
			target.selectable = false,\
			paragraph.leftIndent = 51\
			),\
		paragraph showing (\
			show '.AVI, .MOV, .BMP, .GIF, .JPG, .PNG, .MP3'\
			) with (\
			edit.selectWholeTarget = true,\
			target.selectable = false,\
			paragraph.leftIndent = 65\
			),\
\
		lead with height = 7,\
\
		row showing (\
			show 'Caption:' with (\
				text.bold = true,\
				edit.selectWholeTarget = true,\
				target.selectable = false\
				),\
			pane showing editable (\
				pile showing owner of my window using dataEntryMLA ('caption',\
					xxxCode of ^[Global]language.preferredEncodings)\
					with edit.showAllInsertionPoints = true\
				) size (214, 33)\
			) with row.tabs = (51),\
\
		lead with height = 7,\
\
		row showing (\
			space with width = 75,\
\
			button labeled 'OK' performing\
				begin\
					do perform ( { | C | set alternative xxxCode of encoding of ^C for caption  of object\
							of ^window to ^C} ) to choose all ... for caption of ^application\
					set path of object of ^window to path of ^application\
					do close (true) to ^window\
				end,\
\
			space with width = 7,\
\
			regenerate run showing (\
				if exists of path of my application then\
					lit. Icon() using shellLinkButton( path of my application )\
				else\
					lit. Icon() using shellLinkButton( lit. none, 'wav' )\
				)\
				on changes to path of my application,\
\
			space with width = 7,\
\
			button labeled 'Cancel' performing\
				do close to ^window\
			) with (\
				button.minWidth = 52,\
				text.bold = true\
				)\
\
		) with text [font = Boilerplate, size = 9]\
		without scrolling background gray\
		at (0%, 0%) size (100%, 100%)\
\
	)\
\
	owning attributes are (\
		caption : multilingual String,\
		path : PathName\
		)\
\
	initialize with\
		begin\
			do perform ( { | C | set alternative xxxCode of encoding of ^C for caption of my owner to ^C} )\
				to choose all ... for caption of my object\
			set path of my owner to if exists of path of my object\
				then path of my object \
			set object of pane ('main') of self to my object\
		end\
	initial size (300, 0)\
	default button is OK\
	cancel button is Cancel\
	title is 'Media File Chooser'
