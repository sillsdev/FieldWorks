val August-15-2001 19:42:46 /*  methods for: editor : an ApplicationDefn of FieldManual ***************************\
Change History:\
	6 May 98 MBH Copied from MajorDocElement editor.\
	11 May 98 MBH Modified onOpen\
	22 Jul 98 MBH Added toggleForLimitedAudience\
	4 Aug 98 MBH Modified initializationCustom\
	21 Aug 98 MBH Modified 'onOpen', 'shutDown'\
	27 Aug 98 MBH Removed 'parseNewObject' and 'loadDumpFile' as they are\
			  inherited from DocumentProcessor\
	6 Nov 98 MBH Modified 'fileExport'\
	22 Mar 99 MBH Modified 'fileExport'\
***********************************************************************************************/\
methods are (\
\
/* initializationCustom **********************************************************************\
Description: This method is called from 'initialization' of DocumentProcessor\
Change history:\
	4 Aug 98 MBH Added setting of tableManager\
***********************************************************************************************/\
initializationCustom : means begin\
	do initializeOutline to self\
	do initializeHistory to self\
	do processApplicationStyle to self\
	set my tableManager to create of !TextualTableManager\
	set visible of toolBar('tableBar') of my window to false\
	end,\
\
\
/* fileImport *********************************************************************************\
***********************************************************************************************/\
	fileImport : means begin\
		var format\
	\
		format := chooseOne('Select which Format:') over \
			['CELLAR Format', 'SGML File']\
		if (^format = 'CELLAR Format') then\
			do loadDumpFile to self\
		else if (^format = 'SGML File') then\
			do parseNewObject( 'sgmlModulesFirst' ) to self\
		end,\
\
/* fileExport *********************************************************************************\
	6 Nov 98 MBH Added RTF\
	12 Nov 98 MBH Added HtmlHelp and Web\
	22 Mar 99 MBH Removed option  'HTML for Internet',\
							  Added option 'HTML for SIL Website'\
***********************************************************************************************/\
	fileExport : Missing means begin\
		var vOptions, vChoice, vIndex\
\
		vOptions := [\
			'Cellar Format (.CLR)', \
			'Current View as Text (.TXT)',\
			'RTF (Rich Text Format and WinHelp)',\
			'HTML for Windows Help page(s)',\
			if exists over selectWhere( 'class', '=', !SILWebBuilder ) over\
					workInProcess of workshop of my object then\
				'HTML for SIL Website'\
			]\
		vChoice := chooseOne('Select which Format:') over ^vOptions\
		vIndex := positionOf( ^vChoice ) over ^vOptions\
		if ( ^vIndex = 1 ) then\
			do saveToDumpFile to object of textPane of self\
		else if ( ^vIndex = 2 ) then\
			do saveViewToFile to textPane of self\
		else if ( ^vIndex = 3 ) then\
			do exportAsRTFfile( true ) to object of my textPane\
		else if ( ^vIndex = 4 ) then\
				do exportAsHtmlHelpFile to object of my textPane\
		else if ( ^vIndex = 5 ) then\
				do exportAsWebFile to object of my textPane\
		end,\
\
/* onOpen ***********************************************************************************\
Description: Things to do after the window has opened. Process some attributes\
of the DocumentProcessorPropertySet in the settings of my applicationStyle,\
specifically autoStartOutline and showWhatsNewOnStartup.\
\
Change History:\
	11 May 98 MBH Modify processing of whatsNew stuff\
	21 Aug 98 MBH Added the setting of window size  fudge factors.\
***********************************************************************************************/\
	onOpen : means begin\
		var vDocProPS, vWhatsNewDoc\
	   set activePane of my window to my textPane\
		vDocProPS := selectWhere( 'class', '=', !DocumentProcessorPropertySet )\
			over settings of my applicationStyle\
\
		/* decide whether to automatically start outline */\
		if autoStartOutline of ^vDocProPS then\
			do toolsShowOutline to self\
\
		/* if my whatsNewDoc has changed, open it */\
		vWhatsNewDoc := my whatsNewDoc\
/*        if ( exists of ^vWhatsNewDoc and\
					( whatsNewAutoStartDate of ^vDocProPS << modified of ^vWhatsNewDoc ) ) then begin\
			do showWhatsNew to self\
			set whatsNewAutoStartDate of ^vDocProPS to \
					/* today */\
					create of !TimeStamp\
			end\
*/ lit. missing\
		/* set window size fudge factors */\
		set my windowWidthFudgeFactor to \
			( initialWindowWidth of ^vDocProPS - width of my window )\
		set my windowHeightFudgeFactor to \
			( initialWindowHeight of ^vDocProPS - height of my window )\
\
		end,\
\
/* onRightClick *****************************************************************************\
Description: The action to perform when the user right-clicks in the textPane\
Users:\
	Called from virtual textPaneDefn\
Change History:\
	6 May 98 MBH Created.\
***********************************************************************************************/\
onRightClick : means\
		do popupMenu( my textPane) to self,\
\
/* shutDown ********************************************************************************\
Change history:\
	21 Aug 98 MBH Incorpoarted window size fudge factors\
***********************************************************************************************/\
shutDown: means begin\
	var vDPPS\
	do close to my outlineWindows\
	do gotoShutdown to self\
	vDPPS := selectWhere( 'class', '=', !DocumentProcessorPropertySet ) over\
		settings of my applicationStyle\
	if exists over ^vDPPS then begin\
		set initialWindowHeight of ^vDPPS to \
			( height of my window + my windowHeightFudgeFactor )\
		set initialWindowWidth of ^vDPPS to \
			( width of my window + my windowWidthFudgeFactor )\
		end\
	end,\
\
toggleForLimitedAudience : means begin\
	var vSel\
	vSel := selection of my textPane\
	if ( class of ^vSel = !ObjectSelection ) then\
		do try( 'toggleForLimitedAudience' ) to selectedObjects of ^vSel\
	else\
		begin    \
			var vObjects, vDocElement\
			vObjects := containingObject of allSelections of ^vSel\
			vDocElement := first over selectIf( { | OBJ |\
				( class of attrDefnFor('forLimitedAudience') of class of ^OBJ = !OwningAttrDefn )\
				} ) of ^vObjects\
		if exists over ^vDocElement then\
			do toggleForLimitedAudience to ^vDocElement\
		else\
			do toggleForLimitedAudience to object of my textPane\
		end\
	do redraw to my textPane\
	end\
)
