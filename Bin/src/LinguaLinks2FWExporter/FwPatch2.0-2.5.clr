val March-25-2002 15:7:19 shellLinkButton( \
	pFile : PathName default is lit. missing, \
	pExtension : String default is lit. missing\
	) : frame showing (\
		if exists over ^pExtension then\
			buttonFor( ^pExtension ) of self\
		else if exists over ^pFile then\
			buttonFor( asLowercase of fileExtension of ^pFile ) of self\
		else\
			my linglinkButton\
	) with (\
		frame [\
			insideMargin=1, outsideMargin=0, minWidth=9,\
			borderStyle='roundedCorner',\
			backColor=gray,\
			verticalOffset=0,\
