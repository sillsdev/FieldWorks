val January-29-99 9:12:30 webExportProperties : application subclass of ModalDialogApplication showing (\
	pane 'main' showing (\
		for all APP in my application show pile showing (\
			lead with height=5\
			^APP using objectCombo,\
			lead with height=7\
			^APP using buttonsAndBoxes\
			^APP using buttonRow\
			)\
		)\
		background gray\
		without scrolling\
	)\
\
owning attributes are (\
	canCancel : Boolean default is lit. true,\
	initialOption : Text,\
	initialUnderConstruction : Boolean,\
\
/* option **************************************************************************************\
Description: values are 'forWeb', 'contentsOnly', 'notForWeb'\
************************************************************************************************/\
	option : Text\
	)\
\
reference attributes are (\
	focusObject : AuthorableElement,\
	comboBoxObject : AuthorableElement\
	)\
\
virtual attributes are (\
	calculatedOption : Text means begin\
		var vObject\
		vObject := my focusObject\
		if forWebVersion of ^vObject then\
			'forWeb'\
		else if ( accepts( ^vObject ) of !Document and \
				forWebVersionContents of ^vObject ) then\
			'contentsOnly'\
		else if not of forWebVersion of ^vObject then\
			'notForWeb'\
		else\
			lit. missing\
		end\
	)\
\
title is 'Web Export Properties'\
initial size (250,0)\
initialize with \
	do initialization to my owner\
default button is OK\
cancel button is Cancel
