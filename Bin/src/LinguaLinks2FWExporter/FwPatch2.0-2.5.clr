val January-28-99 20:14:50 objectCombo : \
	combo-box showing (space with width=20) with comboBox [\
		target=self,\
		attrName=comboBoxObject,\
		items=ownersUpTo( topDocument of param1 of ^application ) of \
			param1 of ^application,\
		width=200,\
		clickAction=begin\
			var vApp, vOptionsChanged\
			vApp := ^application\
			vOptionsChanged := ( ( option of ^vApp ~= initialOption of ^vApp ) or\
					( underConstruction of focusObject of ^vApp ~=  initialUnderConstruction of ^vApp ) ) \
			if ( ( comboBoxObject of ^vApp ~= focusObject of ^vApp ) and ^vOptionsChanged ) then\
				set canCancel of ^vApp to false\
			set focusObject of ^application to comboBoxObject of ^application\
			set option of ^vApp to calculatedOption of ^vApp\
			set initialOption of ^vApp to option of ^vApp\
			do redraw to ^pane\
			end\
		]
